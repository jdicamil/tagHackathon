<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.att.m2x.testapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.att.m2x.testapp.FeedsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.att.m2x.testapp.FeedDetailActivity"
            android:label="@string/title_activity_feed_detail" >
        </activity>
        <activity
            android:name="com.att.m2x.testapp.Player"
            android:label="@string/title_activity_player" >
        </activity>
        <activity
            android:name="com.att.m2x.testapp.Maps"
            android:label="@string/title_activity_maps" >
        </activity>
        <activity
            android:name="com.att.m2x.testapp.Stats"
            android:label="@string/title_activity_stats" >
        </activity>
        <activity
            android:name="com.att.m2x.testapp.Tts"
            android:label="@string/title_activity_tts" >
        </activity>
    </application>

</manifest>
